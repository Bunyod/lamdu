{
    "maxExprDepth": 40,
    "debug": {
        "debugShowFPS": false,
        "virtualCursorShown": false
    },
    "zoom": {
        "shrinkKeys"    : [ "Cmd+Minus" ],
        "enlargeKeys"   : [ "Cmd+Equal" ],
        "shrinkFactor"  : 1.1,
        "enlargeFactor" : 1.1
    },
    "export": {
        "exportPath"      : "export.json",
        "exportKeys"      : [ "Alt+E" ],
        "exportFancyKeys" : [ "Alt+Shift+J" ],
        "exportAllKeys"   : [ "Alt+Shift+E" ],
        "executeKeys"     : [ "Cmd+Alt+Shift+R" ],
        "importKeys"      : [ "Alt+I" ]
    },
    "pane": {
        "newDefinitionKeys"            : [ "Alt+N", "Alt+L" ],
        "paneCloseKeys"                : [ "Alt+W" ],
        "paneMoveUpKeys"               : [ "Alt+Up" ],
        "paneMoveDownKeys"             : [ "Alt+Down" ]
    },
    "versionControl": {
        "undoKeys"           : [ "Cmd+Z" ],
        "redoKeys"           : [ "Cmd+Y" ],
        "makeBranchKeys"     : [ "Cmd+S" ],
        "delBranchKeys"      : [ "Alt+O" ],
        "jumpToBranchesKeys" : [ "Cmd+F11" ]
    },
    "completion": {
        "completionJumpToNextKeys"  : [ "Tab" ],
        "completionResultCount"     : 5,
        "completionJumpToPrevKeys"  : [ "Shift+Tab" ],
        "completionOpenKeys"        : [ "Space", "Enter" ],
        "completionCloseKeys"       : [ "Escape", "Cmd+LeftBracket" ]
    },
    "literal": {
        "literalStartEditingKeys" : [ "Enter", "Shift+Right" ],
        "literalStopEditingKeys"  : [ "Escape", "Shift+Left" ]
    },
    "eval": {
        "prevScopeKeys": [ "Cmd+Up" ],
        "nextScopeKeys": [ "Cmd+Down" ]
    },
    "menu" : {
        "keysPickOption": [ "Enter" ],
        "keysPickOptionAndGotoNext": [ "Space" ]
    },
    "helpKeys": [ "F1", "Alt+H" ],
    "previousCursorKeys"       : [ "Alt+Left" ],
    "enterSubexpressionKeys"   : [ "Shift+Right" ],
    "actionKeys"               : [ "Space", "Enter" ],
    "delBackwardKeys"          : [ "Backspace" ],
    "quitKeys"                 : [ "Cmd+Q" ],
    "changeThemeKeys"          : [ "Alt+T" ],
    "addNextParamKeys"         : [ "Space" ],
    "paramOrderBeforeKeys"     : [ "Alt+Up" ],
    "paramOrderAfterKeys"      : [ "Alt+Down" ],
    "attachKeys"               : [ "Space", "Enter", "Shift+0", "RightBracket" ],
    "detachKeys"               : [ "Space" ],
    "parenDetachKeys"          : [ "Shift+9", "LeftBracket" ],
    "recordOpenKeys"           : [ "Alt+O" ],
    "recordCloseKeys"          : [ "Alt+O" ],
    "caseOpenKeys"             : [ "Alt+O" ],
    "extractKeys"              : [ "Cmd+X", "Alt+Up" ],
    "moveLetInwardKeys"        : [ "Alt+Down" ],
    "inlineKeys"               : [ "Cmd+V" ],
    "nextInfoModeKeys"         : [ "F7" ],
    "leaveSubexpressionKeys"   : [ "Shift+Left" ],
    "recordAddFieldKeys"       : [ "Cmd+A", "Comma" ],
    "caseAddAltKeys"           : [ "Cmd+A", "Comma" ],
    "delForwardKeys"           : [ "Delete", "Alt+Delete" ],
    "replaceParentKeys"        : [ "Cmd+Delete", "Cmd+Backspace" ],
    "letAddItemKeys"           : [ "Alt+L" ],
    "jumpToDefinitionKeys"     : [ "Enter" ]
}
